<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <changeSet author="rvolo" id="test">
        <createTable tableName="test_entity">
            <column name="id" type="bigint">
                <constraints nullable="false"/>
            </column>
            <column name="test_field1" type="VARCHAR(255)"/>
            <column name="test_field2" type="VARCHAR(255)"/>
        </createTable>

        <addPrimaryKey columnNames="id" constraintName="PRIMARY" tableName="test_entity"/>
        <addAutoIncrement tableName="test_entity" columnName="id" columnDataType="bigint"/>

    </changeSet>


</databaseChangeLog>
